{
  "manifest_version": 3,
  "name": "Open Multiple URLs",
  "version": "1.0",
  "description": "Efficiently open and manage multiple URLs in Chrome. Batch open links, organize them into groups, and boost your productivity.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "author": "ahkjxy",
  "homepage_url": "https://github.com/ahkjxy/open-multiple-urls"
}